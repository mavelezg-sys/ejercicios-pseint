// 07_mayor_de_dos.psc
Algoritmo MayorDeDos
    Definir a, b Como Real
    Escribir "Número A:"; Leer a
    Escribir "Número B:"; Leer b
    Si a = b Entonces
        Escribir "Son iguales."
    SiNo
        Si a > b Entonces
            Escribir "El mayor es A = ", a
        SiNo
            Escribir "El mayor es B = ", b
        FinSi
    FinSi
FinAlgoritmo
